{
    "name": "mchev/laravel-odk",
    "description": "Laravel-odk is a simple wrapper around the ODK Central API",
    "keywords": [
        "mchev",
        "laravel-odk",
        "odk-central",
        "laravel",
        "wrapper",
        "api",
        "odk"
    ],
    "homepage": "https://github.com/mchev/laravel-odk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Martin Chevignard",
            "email": "martin.chevignard@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mchev\\LaravelOdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mchev\\LaravelOdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mchev\\LaravelOdk\\Providers\\OdkCentralServiceProvider"
            ],
            "aliases": {
                "OdkCentral": "Mchev\\LaravelOdk\\Facades\\OdkCentralFacade"
            }
        }
    }
}
